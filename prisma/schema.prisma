// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       String @id @default(uuid())
  name     String
  email    String @unique
  password String
}

model createUsers {
  id       String @id @default(uuid())
  name     String
  email    String @unique
  password String
}

model ScheduleMetting {
  id       String   @id @default(uuid())
  name     String
  email    String
  country  String
  state    String
  city     String
  checkIn  DateTime
  checkOut DateTime
}

model eventBooking {
  id       String @id @default(uuid())
  email    String
  checkIn  String
  checkOut String
}


model feedback_calendar {
 id           String    @id @default(uuid())
 user_id      String
 user_email   String
 feedback     String
 user_name    String
 rating       Rating[]
}

model Rating {
 id            String        @id @default(uuid())
 rating_user_id String 
 createdAt     DateTime      @default(now())
 updatedAt     DateTime      @updatedAt
 rating_message String
 user_rating String
 feedback_user feedback_calendar @relation(fields: [rating_user_id], references: [id])
}

