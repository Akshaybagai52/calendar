// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       String @id @default(uuid())
  name     String
  email    String @unique
  password String
}

model createUsers {
  id       String @id @default(uuid())
  name     String
  email    String @unique
  password String
}

model ScheduleMetting {
  id       String   @id @default(uuid())
  name     String
  email    String
  country  String
  state    String
  city     String
  checkIn  DateTime
  checkOut DateTime
}

model eventBooking {
  id       String   @id @default(uuid())
  email    String
  checkIn  String
  checkOut String
}

model Feedback_user{
  id String  @id @default(uuid())
  user_id String
  user_email String
  feedback String
  user_name String 
}
